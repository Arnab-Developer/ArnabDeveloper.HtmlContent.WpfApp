<Window x:Class="ArnabDeveloper.HtmlContent.WpfApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="MainWindow" Height="400" Width="500">
    <StackPanel>
        <Button Name="btnGetContent" Content="GetContent" Click="BtnGetContent_Click" />
        <Button Name="btnGetContentAsync" Content="GetContentAsync" Click="BtnGetContentAsync_Click" />
        <Button Name="btnGetContentAsyncStream" Content="GetContentAsyncStream" 
                Click="BtnGetContentAsyncStream_Click" />
        <Button Name="btnGetContentParallelAsync" Content="GetContentParallelAsync" 
                Click="BtnGetContentParallelAsync_Click" />
        <Button Name="btnGetContentParallelForEachAsync" Content="GetContentParallelForEachAsync" 
                Click="BtnGetContentParallelForEachAsync_Click" />
        <Button Name="btnGetContentParallelForEachProgressAsync" Content="GetContentParallelForEachProgressAsync" 
                Click="BtnGetContentParallelForEachProgressAsync_Click" />
        <ProgressBar Name="prg" Height="20" />
        <TextBlock Name="txtResult" />
    </StackPanel>
</Window>
